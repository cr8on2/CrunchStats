<h1>New Fundinground</h1>


<%= form_tag(fundingrounds_url, method: 'post') do %>


  <div>
    <%= label_tag :series %><br />
    <%= text_field_tag :series %>
  </div>

  <div>
    <%= label_tag :amount %><br />
    <%= text_field_tag :amount %>
  </div>

  <div>
    <%= label_tag :currency %><br />
    <%= text_field_tag :currency %>
  </div>

  <div>
    <%= label_tag :date %><br />
    <%= text_field_tag :date %>
  </div>

  <div>
    <%= label_tag :entity_receiver_id %><br />
    <%= number_field_tag :entity_receiver_id %>
  </div>

  <div>
    <%= label_tag :investment_id %><br />
    <%= number_field_tag :investment_id %>
  </div>


  <div>
    <%= submit_tag "Create Fundinground" %>
  </div>
<% end %>
